{
  "name": "ekscss",
  "version": "0.0.0",
  "description": "A simple, fast, and powerful CSS preprocessor",
  "license": "MIT",
  "repository": "MaxMilton/ekscss",
  "author": "Max Milton <max@wearegenki.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn prebuild && NODE_ENV=production yarn node build.mjs && yarn postbuild",
    "dev": "NODE_ENV=development yarn node build.mjs",
    "postbuild": "tsc --emitDeclarationOnly",
    "prebuild": "rm -rf dist"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@types/stylis": "^4.0.0",
    "source-map": "^0.7.3",
    "stylis": "^4.0.10"
  },
  "devDependencies": {
    "esbuild": "0.11.9",
    "typescript": "4.2.4"
  }
}
