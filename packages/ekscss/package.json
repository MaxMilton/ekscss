{
  "name": "ekscss",
  "version": "0.0.8",
  "description": "A simple, fast, and powerful CSS preprocessor",
  "repository": "maxmilton/ekscss",
  "homepage": "https://maxmilton.github.io/ekscss",
  "author": "Max Milton <max@happycode.ltd>",
  "license": "MIT",
  "engines": {
    "node": ">= 10"
  },
  "main": "dist/index.js",
  "browser": "dist/browser.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pnpm run prebuild && NODE_ENV=production node build.mjs && pnpm run postbuild",
    "dev": "NODE_ENV=development node build.mjs",
    "postbuild": "tsc --emitDeclarationOnly",
    "prebuild": "rm -rf dist"
  },
  "dependencies": {
    "@types/stylis": "^4.0.1",
    "source-map": "^0.7.3",
    "stylis": "^4.0.10"
  },
  "devDependencies": {
    "esbuild": "0.12.26",
    "typescript": "4.4.2"
  }
}
