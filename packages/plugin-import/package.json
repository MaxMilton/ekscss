{
  "name": "@ekscss/plugin-import",
  "version": "0.0.9",
  "description": "XCSS plugin to inline the contents of @import statements",
  "repository": "maxmilton/ekscss",
  "homepage": "https://ekscss.js.org",
  "author": "Max Milton <max@happycode.ltd>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run prebuild && node build.mjs && pnpm run postbuild",
    "dev": "NODE_ENV=development node build.mjs",
    "postbuild": "tsc --emitDeclarationOnly",
    "prebuild": "rm -rf dist"
  },
  "peerDependencies": {
    "ekscss": "workspace:^"
  },
  "dependencies": {
    "stylis": "^4.1.3"
  },
  "devDependencies": {
    "@types/stylis": "4.0.2",
    "ekscss": "workspace:*",
    "typescript": "4.9.5"
  }
}
