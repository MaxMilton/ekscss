{
  "name": "svelte-ekscss",
  "version": "0.0.11",
  "description": "Svelte plugin to add support for XCSS",
  "repository": "maxmilton/ekscss",
  "homepage": "https://ekscss.js.org",
  "author": "Max Milton <max@happyhackers.xyz>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run prebuild && node build.mjs && pnpm run postbuild",
    "dev": "NODE_ENV=development node build.mjs",
    "postbuild": "tsc --emitDeclarationOnly",
    "prebuild": "rm -rf dist"
  },
  "peerDependencies": {
    "ekscss": "workspace:^",
    "svelte": "^3.0.0 || ^4.0.0"
  },
  "dependencies": {
    "joycon": "^3.1.1",
    "kleur": "^4.1.5"
  },
  "devDependencies": {
    "ekscss": "workspace:*",
    "svelte": "4.2.9",
    "typescript": "5.3.2"
  }
}
